{
  "name": "simple-timeout",
  "version": "0.1.7",
  "description": "Simple timeout for easy handling in Node.js developed in TypeScript.",
  "keywords": [
    "timeout",
    "simple timeout",
    "simple-timeout",
    "simpletimeout"
  ],
  "homepage": "https://github.com/bloodred17/simple-timeout#readme",
  "bugs": {
    "url" : "https://github.com/bloodred17/simple-timeout/issues",
